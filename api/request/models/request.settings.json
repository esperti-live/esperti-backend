{
  "kind": "collectionType",
  "collectionName": "requests",
  "info": {
    "name": "request"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "requests"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "tags": {
      "via": "requests",
      "collection": "tag",
      "dominant": true
    }
  }
}
