{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "name": "profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "title": {
      "type": "text"
    },
    "video_url": {
      "type": "string"
    },
    "bio": {
      "type": "richtext"
    },
    "skills": {
      "collection": "skill",
      "via": "profile"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "profile"
    }
  }
}
